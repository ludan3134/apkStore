{
  "compilerOptions": {
    "downlevelIteration": true
    /** This fixes error when converting numbers to array of numbers*/,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": [
      "WebWorker",
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true,
    "noImplicitAny ": true,
    "paths": {
      "*": [
        "node_modules/*"
      ]
      //      "@/*": [
      //        "src/*"
      //      ]
    },
    //    "declaration": true,
    "strict": false,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "types": [
      "vite/client",
      "node"
    ]
  },
  "include": [
    "src",
    "vite.config.ts"
  ]
}
